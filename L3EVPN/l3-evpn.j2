vrf definition {{ vrf }}
!
ip routing vrf {{ vrf }}
!
interface {{ interface }}
   vrf forwarding {{ vrf }}
   ip address {{ ipaddress }}/{{ netmask }}
!
interface vxlan1
   vxlan vrf {{ vrf }} vni {{ vrfid }}
!
router bgp {{ asnumber }}
   vrf {{ vrf }}
      rd {{ asnumber }}:{{ vlanid }}
      route-target both {{ asnumber }}:{{ vrfid }}
      redistribute connected
